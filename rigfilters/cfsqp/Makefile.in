#################################################################
# @LICENSE@
#################################################################

SHELL  = /bin/sh

## your compiler and compiler flags
#
CC     = @CC@
CFLAGS = @CFLAGS@

## other defined flags. 
#  DEFS contains stuff that autoconf decides on. 
#  LIBS contains system libraries that the configure script decides we need.
#
DEFS  = @DEFS@
LIBS  = @LIBS@ 

## archiving command, and ranlib command.
# ranlib is used to create the cfsqp.a library, necessary for cm2hmm
#
AR     = @AR@ rcv 
RANLIB = @RANLIB@

OBJS  = cfsqp.o\
	qld.o

HDRS  = cfsqpusr.h\

.c.o: 
	$(CC) $(CFLAGS) $(DEFS) -c $<

#################################################################
## Targets used in making cfsqp library.
##
all: cfsqp.a

cfsqp.a: $(OBJS) ${HDRS}
	$(AR) cfsqp.a $(OBJS) 
	$(RANLIB) cfsqp.a
	chmod 644 cfsqp.a


#################################################################
## Miscellaneous targets.
##
distclean:
	make clean
	-rm -f *.c *.h
	-rm -f Makefile

clean:
	-rm -f *.o *~ cfsqp.a

