#######################################
# cmbuild exercises
#
1  exercise    cmbuild:help/1     @src/cmbuild@ -h
1  exercise    cmbuild:help/2     @src/cmbuild@ --devhelp
1  exercise    cmbuild/trna       @src/cmbuild@ -F %TRNA.CM% !testsuite/tRNA.rfam10p1.seed.sto!
1  exercise    cmbuild/vault      @src/cmbuild@ -F %VAULT.CM% !testsuite/Vault.rfam10p1.seed.sto!
1  exercise    cmbuild/srp        @src/cmbuild@ -F %SRP.CM% !testsuite/Plant_SRP.rfam10p1.seed.sto!
1  exercise    cmbuild/multi      @src/cmbuild@ -F %3.CM% !testsuite/3.rfam10p1.seed.sto!
#######################################

#######################################
# cmsearch exercises (CM must be calibrated)
1  exercise  cmsearch:help/1      @src/cmsearch@ -h
1  exercise  cmsearch:help/1      @src/cmsearch@ --devhelp
1  prep      copy/trnaC           cp !testsuite/tRNA.calibrated.cm! %TRNA.C.CM%
1  prep      copy/vaultC          cp !testsuite/Vault.calibrated.cm! %VAULT.C.CM%
1  prep      copy/srpC            cp !testsuite/Plant_SRP.calibrated.cm! %SRP.C.CM%
1  prep      copy/3C              cp !testsuite/3.calibrated.cm! %3.C.CM%

1  exercise  trna/search          @src/cmsearch@ -Z 1000 %TRNA.C.CM% !testsuite/3trnas.fa!
1  exercise  vault/search         @src/cmsearch@ -Z 1000 %VAULT.C.CM% !testsuite/3trnas.fa!
1  exercise  srp/search           @src/cmsearch@ -Z 1000 %SRP.C.CM% !testsuite/3trnas.fa!
1  exercise  multi/search         @src/cmsearch@ -Z 1000 %3.C.CM% !testsuite/3trnas.fa!
#######################################

#######################################
# cmstat exercises 
1  exercise  cmstat:help/1        @src/cmstat@ -h
1  exercise  trna/stat            @src/cmstat@   %TRNA.CM% 
1  exercise  vault/stat           @src/cmstat@   %VAULT.CM% 
1  exercise  srp/stat             @src/cmstat@   %SRP.CM% 
1  exercise  trnaC/stat           @src/cmstat@   %TRNA.C.CM% 
1  exercise  vaultC/stat          @src/cmstat@   %VAULT.C.CM% 
1  exercise  srpC/stat            @src/cmstat@   %SRP.C.CM% 
