#######################################
# cmpress exercises (cmscan requires a press'd CM file)
# cleanup after pressing non-calibrated CMs, we won't use these in cmscan
1  exercise  cmpress:help/1       @src/cmpress@ -h
1  exercise  trna/press           @src/cmpress@ %TRNA.CM%
1  prep      cleanup/trna         rm -f %TRNA.CM%.*
1  exercise  vault/press          @src/cmpress@ %VAULT.CM%
1  prep      cleanup/vault        rm -f %VAULT.CM%.*
1  exercise  srp/press            @src/cmpress@ %SRP.CM%
1  prep      cleanup/srp          rm -f %SRP.CM%.*
1  exercise  multi/press          @src/cmpress@ %3.CM%
1  prep      cleanup/multi        rm -f %3.CM%.*

# do not cleanup after these, cmscan will use them
1  exercise  trnaC/press          @src/cmpress@ %TRNA.C.CM%
1  exercise  vaultC/press         @src/cmpress@ %VAULT.C.CM%
1  exercise  srpC/press           @src/cmpress@ %SRP.C.CM%
1  exercise  multiC/press         @src/cmpress@ %3.C.CM%
#######################################

#######################################
# cmscan exercises (cmscan requires a press'd CM file with E-values)
1  exercise  cmscan:help/1        @src/cmscan@ -h
1  exercise  cmscan:help/2        @src/cmscan@ --devhelp
1  exercise  trnaC/scan           @src/cmscan@ -Z 1000 %TRNA.C.CM% !testsuite/3trnas.fa!
1  prep      cleanup/trnaC        rm -f %TRNA.C.CM%.*
1  exercise  vaultC/scan          @src/cmscan@ -Z 1000 %VAULT.C.CM% !testsuite/3trnas.fa!
1  prep      cleanup/vaultC       rm -f %VAULT.C.CM%.*
1  exercise  srpC/scan            @src/cmscan@ -Z 1000 %SRP.C.CM% !testsuite/3trnas.fa!
1  prep      cleanup/srpC         rm -f %SRP.C.CM%.*
1  exercise  multiC/scan          @src/cmscan@ -Z 1000 %3.C.CM% !testsuite/3trnas.fa!
1  prep      cleanup/multiC       rm -f %3.C.CM%.*
#######################################

